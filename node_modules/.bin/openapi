#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules/openapi-ts-request/dist/bin/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules/openapi-ts-request/dist/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules/openapi-ts-request/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules/openapi-ts-request/dist/bin/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules/openapi-ts-request/dist/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules/openapi-ts-request/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/openapi-ts-request@1.7.0_@t_21df6b378e77674e24b13f72b1888eed/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openapi-ts-request/dist/bin/openapi.js" "$@"
else
  exec node  "$basedir/../openapi-ts-request/dist/bin/openapi.js" "$@"
fi
