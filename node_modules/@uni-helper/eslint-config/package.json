{
  "name": "@uni-helper/eslint-config",
  "type": "module",
  "version": "0.5.0",
  "description": "Uni Helper's ESLint config",
  "author": "KeJun",
  "license": "MIT",
  "homepage": "https://github.com/uni-helper/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/eslint-config.git"
  },
  "bugs": "https://github.com/uni-helper/eslint-config/issues",
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "eslint": "^9.10.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@eslint/eslintrc": "^3.3.1",
    "eslint-flat-config-utils": "^2.1.1",
    "local-pkg": "^1.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.18.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "esno": "^4.8.0",
    "execa": "^9.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit"
  }
}