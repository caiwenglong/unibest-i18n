#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/eslint-config/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/eslint-config/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../../@antfu+eslint-config@4.19.0_b17cae63e4f32bcf1d12e59f2a1ba64d/node_modules/@antfu/eslint-config/bin/index.js" "$@"
fi
