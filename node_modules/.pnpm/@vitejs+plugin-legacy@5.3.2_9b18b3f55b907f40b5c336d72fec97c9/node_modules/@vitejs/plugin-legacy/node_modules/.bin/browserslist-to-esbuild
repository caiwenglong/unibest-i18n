#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules/browserslist-to-esbuild/cli/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules/browserslist-to-esbuild/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules/browserslist-to-esbuild/cli/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules/browserslist-to-esbuild/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules/browserslist-to-esbuild/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../../browserslist-to-esbuild@2.1.1_browserslist@4.25.4/node_modules/browserslist-to-esbuild/cli/index.js" "$@"
fi
