#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/vite-plugin-uni/bin/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/vite-plugin-uni/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/vite-plugin-uni/bin/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/vite-plugin-uni/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules:/mnt/e/github/uniapp/unibest/unibest-i18n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
else
  exec node  "$basedir/../../../../../../@dcloudio+vite-plugin-uni@3_a5d5770ef6b2c292c79ab628ab340d7b/node_modules/@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
fi
